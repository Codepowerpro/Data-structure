  //end
  
  int arr[5]={10,20,30,40,50};
  int item;
  int i=0;
  
  printf("Enter searching item");
  scanf("%d",&item);
  while(i<5)
  {
      
     if(arr[i]==item)
     {
         printf("item found %d at %d possition",item,i);
         break;
     }
     i++;
  }
    if(i>=5)
     {
         printf("Item not found try again");
     }
    return 0;
}
